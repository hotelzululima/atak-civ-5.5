            <filter itemStringName='team' itemStringValue='Cyan'/>
        </overlay>
        <overlay id='friendlyteamgWhite' title='filter_team_white' icon='asset://icons/roles/team.png' iconColor='white' order='8'>
            <filter itemStringName='team' itemStringValue='White'/>
        </overlay>
        <overlay id='friendlyteamYellow' title='filter_team_yellow' icon='asset://icons/roles/team.png' iconColor='yellow' order='9'>
