        </overlay>
        <overlay id='friendlyteamMaroon' title='filter_team_maroon' icon='asset://icons/roles/team.png' iconColor='#FF7F0000' order='13'>
            <filter itemStringName='team' itemStringValue='Maroon'/>
        </overlay>
        <overlay id='friendlyteamPurple' title='filter_team_purple' icon='asset://icons/roles/team.png' iconColor='#FF7F007F' order='14'>
            <filter itemStringName='team' itemStringValue='Purple'/>
        </overlay>
