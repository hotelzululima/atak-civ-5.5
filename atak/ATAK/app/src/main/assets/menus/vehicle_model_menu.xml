<?xml version='1.0' encoding='UTF-8'?>
<menu
    buttonBg='bgs/button.xml'
    buttonRadius='65'
    buttonSpan='40'
    buttonWidth='90' >

    <button angle='-90'
            icon='icons/delete.png'
            onClick='actions/shape_delete.xml'
            disabled='!{${removable}}' />

    <button icon='icons/details.png'
            submenu='menus/send_submenu.xml'
            disableSwap='true'
            disableIconSwap='true'
            onClick='actions/showdetails_shape.xml' />

    <button icon='icons/3d_free_look.png'
            onClick='actions/compass_toggle_free_look.xml'
            dependsOnCapability='capability.3d'
            selected='${freeLook}'/>

    <button disabled='!{${editable}}'
            icon='icons/edit2.png' 
            onClick='actions/vehicle_model_edit.xml' />

    <button
        disabled='!{${editable}}'
        icon='icons/rotate_ac.png'
        onClick='actions/rotate.xml' />

    <button
        disabled='!{${editable}}'
        icon='icons/transparency.png'
        onClick='actions/rubber_sheet_transparency.xml'/>

    <button
        icon='icons/pairing_line.png'
        onClick='actions/pairingline.xml'/>

    <button icon='icons/polar_coord.png'
            onClick='actions/polarCoords.xml'/>


    <button
        disabled='!{${editable}}'
        icon='icons/move.png'
        onClick='actions/rubber_sheet_move.xml' />

</menu>
